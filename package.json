{
  "name": "hardhat-project",
  "version": "0.0.1",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test:no-logs": "HIDE_CONTRACT_LOGS=true hardhat test",
    "test:gas": "HIDE_CONTRACT_LOGS=true REPORT_GAS=true hardhat test",
    "check": "hardhat check && npx solhint contracts/** -w 0",
